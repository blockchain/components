{
  "name": "blockchain-components",
  "version": "0.0.1",
  "description": "Blockchain.com components library",
  "main": "src/index.js",
  "scripts": {
    "start": "start-storybook -p 6006",
    "build": "build-storybook",
    "test": "./node_modules/.bin/jest",
    "test:update": "./node_modules/.bin/jest -u",
    "test:coverage": "./node_modules/.bin/jest --coverage"
  },
  "author": "Guillaume Marquilly",
  "license": "ISC",
  "devDependencies": {
    "@storybook/addon-actions": "^3.4.6",
    "@storybook/addon-backgrounds": "^3.4.6",
    "@storybook/addon-info": "^3.4.6",
    "@storybook/addon-notes": "^3.4.6",
    "@storybook/cli": "^3.4.6",
    "@storybook/react": "^3.4.6",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-jest": "^23.2.0",
    "babel-loader": "^7.1.4",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.4",
    "jest": "^23.2.0",
    "identity-obj-proxy": "^3.0.0",
    "istanbul": "^0.4.5",
    "istanbul-coveralls": "^1.0.3",
    "polished": "^1.9.2",
    "prop-types": "^15.6.1",
    "ramda": "^0.25.0",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "styled-components": "^3.3.2"
  },
  "peerDependencies": {
    "polished": "^1.9.2",
    "prop-types": "^15.6.1",
    "ramda": "^0.25.0",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "styled-components": "^3.3.2"
  },
  "jest": {
    "collectCoverageFrom": ["src/**/*.js"],
    "coverageDirectory": "<rootDir>/coverage/blockchain-wallet-v4",
    "coverageReporters": ["json", "html"],
    "moduleNameMapper": {
      "\\.(pdf|jpg|jpeg|png|gif|eot|otf|svg|ttf|woff|woff2)$": "<rootDir>/node_modules/identity-obj-proxy",
      "\\.(css|scss)$": "<rootDir>/node_modules/identity-obj-proxy"
    },
    "setupFiles": [
      "<rootDir>/config/jest/jest.config.js"
    ],
    "snapshotSerializers": [
      "<rootDir>/node_modules/enzyme-to-json/serializer"
    ],
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest"
    }
  }
}
