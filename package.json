{
  "name": "@blockchain-com/components",
  "version": "2.4.1",
  "description": "Blockchain.com components library",
  "main": "esm/index.js",
  "module": "esm/index.js",
  "scripts": {
    "start": "start-storybook -p 6006",
    "build:watch": "nodemon --watch src --exec npm run build",
    "prebuild": "npm ci",
    "build": "node utils/build.js",
    "prepare": "node utils/build.js",
    "build-storybook": "build-storybook",
    "deploy-storybook": "storybook-to-ghpages",
    "build-icons": "node ./utils/generateIcons",
    "test": "jest --notify --watch",
    "test:update": "jest -u",
    "test:coverage": "jest --coverage",
    "ci:test": "jest",
    "ci:test:coverage": "jest --coverage --no-cache",
    "lint:css": "stylelint './src/**/*.js'",
    "lint:js": "eslint 'src/**/*.js' --max-warnings=0",
    "lint:js:fix": "eslint 'src/**/*.js' --fix",
    "flow:check": "flow check",
    "preflow:coverage": "npx flow stop",
    "flow:coverage": "flow-coverage-report --config flow-coverage-report.config.js",
    "changelog": "git-changelog -t",
    "preversion": "./utils/preversion.sh",
    "postversion": "./utils/postversion.sh",
    "preupdate:badges": "npm run test:coverage && npm run flow:coverage",
    "update:badges": "node utils/updateBadges.js"
  },
  "author": "Blockchain",
  "license": "AGPL-3.0-or-later",
  "publishConfig": {
    "access": "public",
    "scope": "@blockchain-com"
  },
  "sideEffects": false,
  "dependencies": {
    "@babel/runtime": "7.1.2",
    "react-tooltip": "3.8.4"
  },
  "devDependencies": {
    "@babel/cli": "7.1.2",
    "@babel/core": "7.1.2",
    "@babel/plugin-proposal-class-properties": "7.1.0",
    "@babel/plugin-proposal-export-namespace-from": "7.0.0",
    "@babel/plugin-transform-runtime": "7.1.0",
    "@babel/preset-env": "7.1.0",
    "@babel/preset-flow": "7.0.0",
    "@babel/preset-react": "7.0.0",
    "@commitlint/cli": "7.1.2",
    "@commitlint/config-conventional": "7.1.2",
    "@storybook/addon-actions": "4.0.0-rc.1",
    "@storybook/addon-backgrounds": "4.0.0-rc.1",
    "@storybook/addon-info": "4.0.0-rc.1",
    "@storybook/addon-links": "4.0.0-rc.1",
    "@storybook/addon-notes": "4.0.0-rc.1",
    "@storybook/cli": "4.0.0-rc.1",
    "@storybook/react": "4.0.0-rc.1",
    "@storybook/storybook-deployer": "2.3.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.0.1",
    "babel-jest": "23.6.0",
    "babel-loader": "8.0.4",
    "babel-plugin-styled-components": "1.8.0",
    "babel-plugin-transform-react-remove-prop-types": "0.4.18",
    "camelcase": "5.0.0",
    "decompress-zip": "0.3.1",
    "enzyme": "3.6.0",
    "enzyme-adapter-react-16": "1.5.0",
    "enzyme-to-json": "3.3.4",
    "eslint": "5.6.1",
    "eslint-config-prettier": "3.1.0",
    "eslint-config-standard": "12.0.0",
    "eslint-loader": "2.1.1",
    "eslint-plugin-babel": "5.2.1",
    "eslint-plugin-flowtype": "3.0.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-node": "7.0.1",
    "eslint-plugin-prettier": "3.0.0",
    "eslint-plugin-promise": "4.0.1",
    "eslint-plugin-react": "7.11.1",
    "eslint-plugin-standard": "4.0.0",
    "flow-bin": "0.83.0",
    "flow-copy-source": "2.0.2",
    "flow-coverage-report": "0.6.0",
    "git-changelog": "1.1.2",
    "husky": "1.1.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "23.6.0",
    "jest-styled-components": "6.2.1",
    "mkdirp": "0.5.1",
    "ncp": "2.0.0",
    "nodemon": "1.18.4",
    "prettier": "1.14.3",
    "prop-types": "15.6.2",
    "react": "16.5.2",
    "react-dom": "16.5.2",
    "rimraf": "2.6.2",
    "standard": "12.0.1",
    "styled-components": "3.4.9",
    "stylelint": "9.6.0",
    "stylelint-config-recommended": "2.1.0",
    "stylelint-config-standard": "18.2.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-custom-processor-loader": "0.5.0",
    "stylelint-processor-styled-components": "1.5.0",
    "svgo": "1.1.1"
