[ignore]
.*/coverage/.*
.*/esm/.*
.*/storybook-static/.*
.*/utils/.*

[include]

[libs]

[lints]

[options]
suppress_comment= \\(.\\|\n\\)*\\$IgnoreStrictImport
suppress_comment= \\(.\\|\n\\)*\\$IgnoreUnitTest
suppress_comment= \\(.\\|\n\\)*\\$IgnoreLibDef
suppress_comment= \\(.\\|\n\\)*\\$WeakTypeNeeded

[strict]
unclear-type
unsafe-getters-setters
untyped-import
untyped-type-import
sketchy-null
sketchy-number