[declarations]

[untyped]

[ignore]

[include]
.*/src/.*

[libs]

[lints]

[options]
module.system.node.resolve_dirname=node_modules
suppress_comment= \\(.\\|\n\\)*\\$IgnoreStrictImport
suppress_comment= \\(.\\|\n\\)*\\$IgnoreUnitTest
suppress_comment= \\(.\\|\n\\)*\\$IgnoreLibDef
suppress_comment= \\(.\\|\n\\)*\\$WeakTypeNeeded

[strict]
unclear-type
unsafe-getters-setters
untyped-import
untyped-type-import
sketchy-null
sketchy-number

[version]
^0.90.0